                            <interpretation-reason>Standard Threshold &lt;500 </interpretation-reason>
                        </bead-interpretation>
                        <bead-interpretation>
                            <bead-classification>Positive</bead-classification>
                            <classification-entity>Ben the Expert</classification-entity>
                            <interpretation-reason>Gut Feeling</interpretation-reason>
                        </bead-interpretation>
                        <bead-interpretation>
                            <bead-classification>Positive</bead-classification>
                            <ratio-threshold>1.7</ratio-threshold>
                            <classification-entity>Werfen Match-It</classification-entity>
                            <interpretation-reason>MFI/LRA ratio above ratio cut-off/threshold</interpretation-reason>
                        </bead-interpretation>

