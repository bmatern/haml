                        <control-bead-observation>
                            <bead-id>3</bead-id>
                            <serum-id>standard_negative_serum_1</serum-id>
                            <description>(target-bead + negative-serum applied)</description>
                            <bead-raw-data><raw-MFI>16</raw-MFI></bead-raw-data>
